{
  "manifest_version": 2,

  "name": "SoundBlock",
  "description": "Block all html5 sounds.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
    //"default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.js", "soundblock.js"]
    }
  ]
}
